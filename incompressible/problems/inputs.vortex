# simple inputs files for the unsplit CTU hydro scheme

[driver]
max_steps = 2000
tmax = 1.0
cfl = 0.8


[incompressible]

[vis]

dovis = 0
store_images = 1

[io]
basename = vortex_e0_512
n_out = 1


[mesh]
nx = 512
ny = 512
xmin = 0.0
xmax = 2.0
ymin = -1.0
ymax = 1.0

xlboundary = periodic
xrboundary = periodic

ylboundary = periodic
yrboundary = periodic

[vortex]
eps = 0.0
